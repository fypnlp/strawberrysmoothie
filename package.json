{
	"name": "AwesomeApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"cleanup": "rm -rf ios/build; rm -rf android/app/build/; rm -rf ios/Pods/; rm e2e/cucumber-report-ios.json; rm e2e/cucumber-report-android.json; rm yarn-error.log; rm -rf android/.gradle; rm -rf node_modules",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"e2e:ios": "configuration=\"ios\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-ios.json",
		"e2e:android:device": "configuration=\"android:device\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-android.json",
		"e2e:android:emulator": "configuration=\"android:emulator\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-android.json"
	},
	"dependencies": {
		"@react-native-community/checkbox": "^0.4.2",
		"@react-native-community/masked-view": "^0.1.10",
		"@react-native-community/toolbar-android": "^0.1.0-rc.2",
		"@react-navigation/bottom-tabs": "^5.6.1",
		"@react-navigation/drawer": "^5.8.4",
		"@react-navigation/material-bottom-tabs": "^5.2.12",
		"@react-navigation/native": "^5.6.1",
		"@react-navigation/stack": "^5.6.2",
		"react": "^16.9.0",
		"react-devtools": "^4.7.0",
		"react-native": "^0.61.0",
		"react-native-gesture-handler": "^1.6.1",
		"react-native-paper": "^3.10.1",
		"react-native-reanimated": "^1.9.0",
		"react-native-safe-area-context": "^3.0.7",
		"react-native-screens": "^2.9.0",
		"react-native-simple-markdown": "^1.1.0",
		"react-native-vector-icons": "^6.7.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^1.0.0",
		"@types/jest": "^24.0.24",
		"@types/react-native": "^0.62.17",
		"@types/react-native-vector-icons": "^6.4.5",
		"@types/react-test-renderer": "16.9.2",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"babel-jest": "^24.9.0",
		"cucumber": "^6.0.5",
		"cucumber-html-reporter": "^5.2.0",
		"cucumber-pretty": "^6.0.0",
		"detox": "^17.1.0",
		"eslint": "^6.5.1",
		"eslint-plugin-detox": "^1.0.0",
		"jest": "^24.9.0",
		"jest-circus": "^26.1.0",
		"metro-react-native-babel-preset": "^0.58.0",
		"prettier": "^2.0.4",
		"react-test-renderer": "16.11.0",
		"typescript": "^3.8.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}